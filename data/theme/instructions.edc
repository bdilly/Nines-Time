            color: 255 255 255 255; \
        } \
        description { state: "hidden" 0.0; \
            inherit: "default" 0.0; \
            color: 255 255 255 0; \
        } \
    }

#define ADD_MSG_PROG(_step) \
program { name: "show_msg_"##_step; \
   signal: "step_"##_step; \
   action: STATE_SET "default" 0.0; \
   transition: DECELERATE 0.3; \
   target: "instructions_clipper"; \
   after: "show_msg_"##_step##"_2"; \
} \
program { name: "show_msg_"##_step##"_2"; \
   action: STATE_SET "step_"##_step 0.0; \
   target: "instructions_msgs"; \
   after: "show_msg_"##_step##"_3"; \
} \
program { name: "show_msg_"##_step##"_3"; \
   action: STATE_SET "visible" 0.0; \
   transition: ACCELERATE 0.3; \
   target: "instructions_clipper"; \
}

#define ADD_MSG(_step, _msg) \
description { state: "step_"##_step 0.0; \
   inherit: "default" 0.0; \
   text.text: _msg; \
}

#define ADD_DAY(_day) \
description { state: "day_"##_day 0.0; \
   inherit: "default" 0.0; \
   text.text: "Day "##_day; \
}

