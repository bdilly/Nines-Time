   description { state: "default" 0.0; \
      rel1.to: _name"_bg"; \
      rel2.to: _name"_bg"; \
      image.normal: _name"_bt.png"; \
   } \
} \
part { name: _name"_shadow"; \
   type: RECT; \
   clip_to: "ops_clipper"; \
   description { state: "default" 0.0; \
      rel1.to: _name"_bg"; \
      rel2 { \
        to: _name"_bg"; \
