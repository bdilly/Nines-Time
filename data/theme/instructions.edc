#define ADD_FAKE_COMBO(_pos, _value) \
part { name: "combo"##_pos##"_clip"; \
   type: RECT; \
   description { state: "default " 0.0; \
      color: 255 255 255 0; \
   } \
   description { state: "visible" 0.0; \
      inherit: "default" 0.0; \
      color: 255 255 255 255; \
   } \
   description { state: "pop" 0.0; \
      inherit: "default" 0.0; \
      color: 255 255 255 0; \
   } \
} \
part { name: "combo"##_pos; \
   clip_to: "combo"##_pos##"_clip"; \
   type: TEXT; \
   description { state: "default " 0.0; \
      rel1.offset: (80 + _pos * 30) (300 - _pos * 50); \
      color_class: "combo"; \
      text { \
         font: "nines"; \
         size: 20; \
         text: "+"_value; \
      } \
   } \
   description { state: "visible" 0.0; \
      inherit: "default" 0.0; \
   } \
   description { state: "pop" 0.0; \
      inherit: "default" 0.0; \
      text.size: 100; \
      } \
}

#define ADD_FAKE_OPERATION(_name) \
part { name: "operation_"_name; \
    description { state: "default" 0.0; \
        min: 30 30; \
        max: 30 30; \
        rel1.to: "hud_bar"; \
        rel1.relative: 0 1; \
        color: 255 255 255 0; \
        image.normal: _name"_full.png"; \
    } \
    description { state: "visible" 0.0; \
        inherit: "default" 0.0; \
        color: 255 255 255 255; \
    } \
    description { state: "big" 0.0; \
        inherit: "default" 0.0; \
        color: 255 255 255 0; \
        min: 600 600; \
        max: 600 600; \
    } \
} \
program { name: _name"_operation_show"; \
    signal: _name; \
    action: STATE_SET "visible" 0.0; \
    target: "operation_"_name; \
    after: _name"_operation_enlarge"; \
} \
program { name: _name"_operation_enlarge"; \
    action: STATE_SET "big" 0.0; \
    target: "operation_"_name; \
    transition: ACCELERATE 0.5; \
        after: _name"_hide_op"; \
    } \
    program { name: _name"_hide_op"; \
        action: STATE_SET "default" 0.0; \
        target: "operation_"_name; \
    }

#define ADD_DEAD_BUTTON_I(_pos, _name) \
part { name: _name"_bg"; \
   type: RECT; \
   clip_to: "ops_clipper"; \
   description { state: "default" 0.0; \
      min: 40 36; \
      max: 40 36; \
      align: 0 0; \
      rel1.to: "ops_bar"; \
      rel1.offset: (_pos * 40) 0; \
      rel2.to: "ops_bar"; \
      color: 255 255 255 0; \
   } \
} \
part { name: _name; \
   clip_to: "ops_clipper"; \
   description { state: "default" 0.0; \
      rel1.to: _name"_bg"; \
      rel2.to: _name"_bg"; \
      image.normal: _name"_bt.png"; \
   } \
}

#define ADD_BUTTON_I(_pos, _name) \
part { name: _name"_bg"; \
